<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#223344"
                />
            <padding android:top="10dp"
                android:left="10dp"
                android:bottom="10dp"
                android:right="10dp"
                />
            <corners
                android:radius="10dp"
                />
        </shape>
    </item>
    <item >

        <shape android:shape="rectangle">
            <solid android:color="#623466"
                />
            <padding android:top="10dp"
                android:left="10dp"
                android:bottom="10dp"
                android:right="10dp"
                />
            <corners
                android:radius="10dp"
                />
        </shape>

    </item>



</selector>